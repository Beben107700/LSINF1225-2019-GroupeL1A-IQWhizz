<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".home">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="0dp"
        android:layout_height="593dp"
        android:layout_marginBottom="130dp"
        android:contentDescription="@android:string/autofill"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/imagehome" />

    <ImageButton
        android:id="@+id/homeloginbutton"
        android:layout_width="200dp"
        android:layout_height="46dp"
        android:layout_marginStart="104dp"
        android:layout_marginTop="496dp"
        android:background="@drawable/buttonlog_in"
        android:contentDescription="@string/log_in"
        android:onClick="onLogin"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/homesignupbutton"
        android:layout_width="200dp"
        android:layout_height="50dip"
        android:layout_marginStart="104dp"
        android:layout_marginTop="572dp"
        android:background="@drawable/buttonsign_in"
        android:contentDescription="@string/sign_in"
        android:onClick="onSignup"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</android.support.constraint.ConstraintLayout>